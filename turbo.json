{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "cache": true,
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "type-check": {
      "cache": true,
      "outputs": ["tsconfig.tsbuildinfo"]
    },
    "storybook": {
      "cache": false,
      "persistent": true
    }
  }
}
