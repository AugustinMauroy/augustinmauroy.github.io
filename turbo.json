{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "cache": true
    },
    "lint-css": {
      "outputs": [".stylelintcache"],
      "cache": true
    },
    "lint-css:fix": {
      "outputs": [".stylelintcache"],
      "cache": true
    },
    "lint-js": {
      "outputs": [".eslintcache"],
      "cache": true
    },
    "lint-js:fix": {
      "outputs": [".eslintcache"],
      "cache": true
    },
    "prettier:write": {
      "outputs": [".prettiercache"],
      "cache": true
    },
    "prettier:check": {
      "outputs": [".prettiercache"],
      "cache": true
    }
  }
}
