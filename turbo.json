{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"dev": {
			"cache": false
		},
		"build": {
			"outputs": [".next/**", "!.next/cache/**"],
			"cache": true
		},
		"start": {
			"cache": false
		},
		"lint": {
			"outputMode": "full"
		},
		"lint:fix": {
			"outputMode": "full"
		},
		"format": {
			"outputMode": "full"
		},
		"format:fix": {
			"outputMode": "full"
		}
	}
}
