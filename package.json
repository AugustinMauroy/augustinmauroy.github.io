{
  "name": "augustinmauroy.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint-css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint-css:fix": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache --fix",
    "lint-js": "next lint --cache --cache-strategy=content --cache-location=.eslintcache",
    "lint-js:fix": "npm run lint-js -- --fix",
    "test": "jest --cache",
    "test:ci": "jest --ci --coverage --cache",
    "prettier:write": "prettier --write . --cache --cache-location=.prettiercache",
    "prettier:check": "prettier --check . --cache --cache-location=.prettiercache",
    "format": "npm run prettier:write && npm run lint-js:fix && npm run lint-css:fix",
    "storybook": "storybook dev -p 6006 --quiet --no-open",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "~1.0.4",
    "bright": "~0.8.4",
    "classnames": "~2.5.1",
    "js-yaml": "~4.1.0",
    "next": "~14.0.4",
    "next-intl": "~3.4.0",
    "next-mdx-remote": "~4.4.1",
    "next-themes": "~0.2.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-icons": "~4.12.0",
    "turbo": "~1.11.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "~7.6.16",
    "@storybook/addon-interactions": "~7.6.7",
    "@storybook/addon-links": "~7.6.8",
    "@storybook/addon-themes": "~7.6.8",
    "@storybook/nextjs": "~7.6.8",
    "@storybook/react": "~7.6.7",
    "@testing-library/jest-dom": "~6.2.0",
    "@testing-library/react": "~14.1.2",
    "@types/jest": "~29.5.11",
    "@types/node": "~20",
    "@types/react": "~18.2.45",
    "@types/react-dom": "~18.2.18",
    "autoprefixer": "~10.4.16",
    "eslint": "~8",
    "eslint-config-next": "~14.0.4",
    "eslint-plugin-storybook": "~0.6.15",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "postcss": "~8",
    "prettier": "~3.1.1",
    "prettier-plugin-tailwindcss": "~0.5.9",
    "storybook": "~7.6.6",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "tailwindcss": "~3.4.1",
    "typescript": "~5"
  }
}
