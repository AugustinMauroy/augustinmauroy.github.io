{
  "name": "augustinmauroy.github.io",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "node ./scripts/postbuild.js",
    "start": "next start",
    "test:unit": "node --no-warnings --enable-source-maps --import ./node-hooks/react.ts --test **/tests/*.test.tsx **/tests/*.test.ts",
    "test:unit-coverage": "node --no-warnings --import ./node-hooks/react.ts --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=./coverage.lcov --test-reporter=spec --test-reporter-destination=stdout **/tests/*.test.tsx **/tests/*.test.ts",
    "test:watch": "node --no-warnings --enable-source-maps --import ./node-hooks/react.js --test --watch **/tests/*.test.tsx **/tests/*.test.ts",
    "test": "node --run test:unit",
    "biome:format": "biome format --write ./",
    "biome:lint": "biome lint --write ./",
    "biome:ci": "biome ci ./",
    "pre-commit": "node --run biome:format && node --run biome:lint",
    "types:check": "tsc --noEmit",
    "storybook": "BUILD_ENV=storybook storybook dev -p 6006 --quiet --no-open",
    "storybook:build": "BUILD_ENV=storybook storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/react": "~2.2.0",
    "@radix-ui/react-avatar": "~1.1.3",
    "@radix-ui/react-dropdown-menu": "~2.1.6",
    "classnames": "~2.5.1",
    "feed": "~4.2.2",
    "geist": "~1.3.1",
    "next": "~15.2.2",
    "next-intl": "~3.26.3",
    "next-mdx-remote": "~5.0.0",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "rehype-autolink-headings": "~7.1.0",
    "rehype-mdx-code-props": "~3.0.1",
    "rehype-slug": "~6.0.0",
    "shiki": "~3.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@happy-dom/global-registrator": "^17.4.4",
    "@nodejs-loaders/alias": "~2.0.1",
    "@nodejs-loaders/css-module": "~1.1.0",
    "@nodejs-loaders/tsx": "~1.1.1",
    "@storybook/addon-a11y": "~8.6.6",
    "@storybook/addon-essentials": "~8.6.6",
    "@storybook/addon-interactions": "~8.6.6",
    "@storybook/addon-links": "~8.6.6",
    "@storybook/addon-themes": "~8.6.6",
    "@storybook/nextjs": "~8.6.6",
    "@storybook/react": "~8.6.6",
    "@testing-library/dom": "~10.4.0",
    "@testing-library/react": "~16.2.0",
    "@types/node": "~22.13.10",
    "@types/react": "~19.0.10",
    "@types/react-dom": "~19.0.4",
    "autoprefixer": "~10.4.21",
    "husky": "~9.1.7",
    "lint-staged": "~15.4.3",
    "postcss": "^8.5.3",
    "storybook": "~8.6.6",
    "tailwindcss": "~4.1.8",
    "typescript": "~5.8.2"
  },
  "engines": {
    "node": "23.x",
    "npm": "10.x"
  },
  "packageManager": "npm@10.0.0"
}
