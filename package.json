{
  "name": "augustinmauroy.github.io",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "node ./scripts/postbuild.js",
    "start": "next start",
    "test:unit": "node --experimental-test-snapshots --test --import=\"./node-hooks/typescript.js\" --import=\"./node-hooks/react.js\" **/*.test.ts **/*.test.tsx",
    "test:watch": "node --run test:unit -- --watch",
    "test": "node --run test:unit",
    "biome:format": "biome format --write ./",
    "biome:lint": "biome lint --write ./",
    "biome:ci": "biome ci ./",
    "format": "node --run biome:format && node --run biome:lint",
    "type-check": "tsc --noEmit",
    "storybook": "BUILD_ENV=storybook storybook dev -p 6006 --quiet --no-open",
    "storybook:build": "BUILD_ENV=storybook storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/react": "~2.1.5",
    "@radix-ui/react-avatar": "~1.1.0",
    "@radix-ui/react-dropdown-menu": "~2.1.1",
    "classnames": "~2.5.1",
    "feed": "~4.2.2",
    "geist": "~1.3.1",
    "next": "~14.2.13",
    "next-intl": "~3.19.4",
    "next-mdx-remote": "~5.0.0",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "rehype-autolink-headings": "~7.1.0",
    "rehype-mdx-code-props": "~3.0.1",
    "rehype-slug": "~6.0.0",
    "shiki": "~1.18.0",
    "turbo": "~2.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@storybook/addon-a11y": "~8.3.2",
    "@storybook/addon-essentials": "~8.3.2",
    "@storybook/addon-interactions": "~8.3.2",
    "@storybook/addon-links": "~8.3.2",
    "@storybook/addon-themes": "~8.3.2",
    "@storybook/nextjs": "~8.3.2",
    "@storybook/react": "~8.3.2",
    "@testing-library/dom": "~10.4.0",
    "@testing-library/react": "~16.0.1",
    "@types/node": "~20.13.0",
    "@types/react": "~18.3.8",
    "@types/react-dom": "~18.3.0",
    "autoprefixer": "~10.4.20",
    "husky": "~9.1.6",
    "jsdom": "~25.0.1",
    "lint-staged": "~15.2.10",
    "nodejs-loaders": "1.1.0",
    "postcss": "~8.4.47",
    "storybook": "~8.3.2",
    "tailwindcss": "~3.4.11",
    "typescript": "~5.7.2"
  },
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "packageManager": "npm@10.0.0"
}
